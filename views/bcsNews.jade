extends layout

block content
	div
		ul#menuUl
			li: a(href="/") Home
	h1 Bcs News
	
	div
		table
			thead
				tr
					th Title
			tbody
				each d in data
					tr
						td
							li: =d.title

	div
		ul#pages
			if page > 0 
				li: a(href="/bcsNews/?page=" + 
				(page-1)) Prev
			if data.length == 100
				li: a(href="/bcsNews/?page=" + 
				(page+1)) Next